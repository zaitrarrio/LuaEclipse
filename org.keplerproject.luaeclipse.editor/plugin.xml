<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.keplerproject.luaeclipse.editor.LuaEditor"
            default="false"
            icon="img/lua-logo.gif"
            id="org.keplerproject.luaeclipse.editor"
            name="Lua Editor">
         <contentTypeBinding
               contentTypeId="org.keplerproject.luaeclipse.content-type">
         </contentTypeBinding>
      </editor>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.keplerproject.luaeclipse.editor.internal.text.LuaPreferenceInitializer">
      </initializer>
   </extension>

</plugin>
